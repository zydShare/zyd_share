#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_xml2map@1.0.2@xml2map/bin/xml2map" "$@"
  ret=$?
else 
  node  "$basedir/../_xml2map@1.0.2@xml2map/bin/xml2map" "$@"
  ret=$?
fi
exit $ret
