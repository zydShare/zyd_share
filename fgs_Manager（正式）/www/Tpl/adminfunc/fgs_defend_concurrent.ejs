<% include ../admin/look_top.ejs%>
<script>
	lists_width = "800";
	lists_height = "500";
</script>
<!--按钮区-->
<% if(_按钮权限.indexOf("fgs_defend_concurrent_insert")>=0){ %>
<div class="img-button img-2">
	<p><input type="button" value="添加分_并发处理" onclick='jq_bottom("1","/Tpl/adminfunc/fgs_defend_concurrent_insert.xhtml?a=1","添加分_并发处理","fgs_defend_concurrent","")' class="addData" /></p>
</div>
<% }%>

<div id="select_button">
	<% if(_按钮权限.indexOf("fgs_defend_concurrent_update")>=0){ %>
	<a v='["1","/Tpl/adminfunc/fgs_defend_concurrent_update.xhtml?a=1","修改分_并发处理","fgs_defend_concurrent",""]'>修改分_并发处理</a>
	<% }%>
	<% if(_按钮权限.indexOf("deleteClasses")>=0){ %>
	<a v='["4","/Tpl/adminfunc/fgs_defend_concurrent_delete.xhtml?a=1","deleteClasses","fgs_defend_concurrent",""]'>删除分_并发处理</a>
	<% }%>
</div>
<div id="search">
	<div id="input"><input type="submit" id="button" value="搜索" class="submit_btn" /><input type="button" value="清空" onclick="jump_html()" class="submit_btn" id="clear" /></div>
	<div id="content">
		<div style="float:left">
			<link type="text/css" rel="stylesheet" href="../../public/date/calendar.css">
			<script type="text/javascript" src="../../public/date/calendar.js"></script>
			<script type="text/javascript" src="../../public/date/calendar-zh.js"></script>
			<script type="text/javascript" src="../../public/date/datelib/newdate.js"></script>
			<script src="../../public/admin/js/aliyun-oss-sdk-4.4.4.min.js" type="text/javascript"></script>
			<link type="text/css" href="../../public/date/datelib/jquery-ui-1.8.17.custom.css" rel="stylesheet" />
			<link type="text/css" href="../../public/date/datelib/jquery-ui-timepicker-addon.css" rel="stylesheet" />
			<script type="text/javascript" src="../../public/date/datelib/jquery-ui-1.8.17.custom.min.js"></script>
			<script type="text/javascript" src="../../public/date/datelib/jquery-ui-timepicker-addon.js"></script>
			<script type="text/javascript" src="../../public/date/datelib/jquery-ui-timepicker-zh-CN.js"></script>
			<script type="text/javascript">
				$(function() {
					$(".ui_timepicker").datetimepicker({
						showSecond: true,
						timeFormat: "hh:mm:ss",
						stepHour: 1,
						stepMinute: 1,
						stepSecond: 1
					})
				})
			</script>
			<div style="float:left;">
				<div style="float:left;">id：</div>
				<input type="text" name="id" value="<%= data.id%>" />&nbsp;&nbsp;</div>
			<div style="float:left;">
				<div style="float:left;">订单id：</div>
				<input type="text" name="订单id" value="<%= data.订单id%>" />&nbsp;&nbsp;</div>
			<div style="float:left;">
				<div style="float:left;">录入时间：</div>
				<input style="height: 30px;padding: 4px 2px;height: 14px;line-height: 14px;border: solid 1px #666;" type="text" id="录入时间" name="录入时间" class="ui_timepicker" value="<%= data.录入时间%>" onclick="return showCalendar('录入时间','y-mm-dd');">&nbsp;&nbsp;</div>
			<br/>

			<div style="float:left;">
				<div style="float:left;">开始日期：</div>
				<input style="height: 30px;padding: 4px 2px;height: 14px;line-height: 14px;border: solid 1px #666;" type="text" id="开始日期" name="开始日期" class="ui_timepicker" value="<%= data.开始日期%>" onclick="return showCalendar('开始日期','y-mm-dd');">&nbsp;&nbsp;</div>
			<div style="float:left;">
				<div style="float:left;">结束日期：</div>
				<input style="height: 30px;padding: 4px 2px;height: 14px;line-height: 14px;border: solid 1px #666;" type="text" id="结束日期" name="结束日期" class="ui_timepicker" value="<%= data.结束日期%>" onclick="return showCalendar('结束日期','y-mm-dd');">&nbsp;&nbsp;</div>

		</div>
	</div>
</div>
</div>
</form>
<div class="search-box"></div>
<table id="checkList" class="table-list list" cellpadding="0" cellspacing="0" border="0" rel="">
	<thead>
		<tr>
			<th class="first" width="30"><input name="" type="checkbox" onclick="dein()" id="howinput" /></th>
			<th width="80px">
				<a onclick="ord('序号')">序号
					<%=arrow['序号']%>
				</a>
			</th>
			<th width="150px">
				<a>操作</a>
			</th>
			<th width="80px">
				<a onclick="ord('id')">id
					<%=arrow['id']%>
				</a>
			</th>
			<th width="300px">
				<a onclick="ord('订单id')">订单id
					<%=arrow['订单id']%>
				</a>
			</th>

			<th width="150px">
				<a onclick="ord('录入时间')">录入时间
					<%=arrow['录入时间']%>
				</a>
			</th>

		</tr>
	</thead>
	<tbody>
		<% var class_name = "class=even";if(r.length == 0){ %>
		<td colspan="15" style="color: #333;line-height: 4;font-size: 16px; border-bottom: 0px solid #FFF;"><img style="width: 25px; vertical-align: middle; margin-right: 10px;" src="../../public/admin/images/close.png" />没有查询到符合条件的记录</td>
		<% }r.forEach(function (name,key){if(class_name == "class=even") class_name = ''; else class_name = "class=even";%>
		<tr <%=class_name%> >
			<td class="first"><input name="howde[]" type="checkbox" value="<%=name.id%>" class="idinput" /></td>
			<td>
				<% var pageNum = Number(arg.page)-1; var sumNum = Number(arg.sumnum); if(sumNum == '' || sumNum == null || !sumNum){sumNum = key+1}else{ sumNum = key + 1 + pageNum*sumNum} %>
				<%= sumNum %>
			</td>
			<td class="up_de" v="<%=name.id%>"><img src="../../public/admin/images/more1.png" class="up_select" /></td>

			<td>
				<%=name.id%>
			</td>
			<td>
				<%=name.订单id%>
			</td>

			<td>
				<%=name.录入时间%>
			</td>

		</tr>
		<% })%>
		<% include ../admin/look_bottom.ejs %>